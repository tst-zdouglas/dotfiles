[color]
	ui = auto
[core]
	editor = emacs
[alias]
	aliases = !git config --get-regexp='alias.*' | colrm 1 6 | sed 's/[ ]/ = /'
	ci = commit
	cim = commit -m
	co = checkout
	up = !git remote update origin && echo && git update
	upup = !git up && git pull
	st = status
	sts = status -s -uno
